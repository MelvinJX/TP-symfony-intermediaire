{% extends "bases_admin.html.twig" %}

{% block main %}
    <main>
        <h2>Produits(admin)</h2>
        <section>
            {% for product  in products %}
                <article>
                    <h3>{{ product.name }}</h3>
                    <h3>{{ product.description }}</h3>
                    <h3>{{ product.price }}</h3>
                    <h3>{{ product.stock }}</h3>
                </article>
            {% endfor %}
        </section>
    </main>
{% endblock %}