{% extends "bases_admin.html.twig" %}

{% block main %}
    <main>
        <h2>Categories(admin)</h2>
        <section>
            {% for category  in categories %}
                <article>
                    <h3>{{ category.name }}</h3>
                    <h3>{{ category.description }}</h3>
                </article>
            {% endfor %}
        </section>
    </main>
{% endblock %}